@article{bib:ident:autoregr,
AUTHOR = {Ivanov, Dmitriy and Yakoub, Zaineb},
TITLE = {Overview of Identification Methods of Autoregressive Model in Presence of Additive Noise},
JOURNAL = {Mathematics},
VOLUME = {11},
